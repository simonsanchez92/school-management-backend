{
  "name": "school-management-backend",
  "version": "1.0.0",
  "description": "School management web application backend",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon -r dotenv/config server.js",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:g:seed": "sequelize-cli seed:generate --name",
    "db:seeds": "sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonsanchez92/school-management-backend.git"
  },
  "keywords": [
    "school",
    "management",
    "backend",
    "administration"
  ],
  "author": "Simon Sanchez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/simonsanchez92/school-management-backend/issues"
  },
  "homepage": "https://github.com/simonsanchez92/school-management-backend#readme",
  "devDependencies": {
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.5"
  }
}
